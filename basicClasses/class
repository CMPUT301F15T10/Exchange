class Person {
    private String Name;
    private String Location;
    private int ID;
    private Picture myPicture;
    private Long int phoneNumber;
    private String userName;
    private Inventory myInventory;
}

class Inventory {
    private int size;
    private ArrayList <Book> myBooks;
}

class Book {
    private String bookName; // Required
    private String genre;
    private int genreID; // Required
    private String author;
    private String publisher;
    private ArrayList <Picture> covers;
    private String ISBN;
    private int condition; // Required
    private String description;
    private IsListed; // Required;
}

class Firend {
    private ArrayList <Person> myFriends;
}

